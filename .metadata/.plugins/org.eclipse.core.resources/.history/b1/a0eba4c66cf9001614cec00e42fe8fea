import java.util.Scanner;

public class max {

	public static void main(String[] args) {
		Scanner read = new Scanner(System.in);
		System.out.println("how many numbers:");
		int b=read.nextInt();
		int a[] = new int[b];
		int max=0;
		for(int i = 0;i<a.length;i++){
			a[i]=read.nextInt();
	}
		for(int i = 0;i<a.length;i++){
			if(max<a[i]){
				max=a[i];
			}
	}
		System.out.println(max);
		

	}

}
